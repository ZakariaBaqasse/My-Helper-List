    <i class="bi bi-list mobile-nav-toggle"></i>
</head>

<body>
    <section id="hero" class="hero d-flex align-items-center">

        <div class="container">
            <div class="row">
                <div class="col-lg-6 d-flex flex-column justify-content-center">
                    <h1 data-aos="fade-up">We Offer A Great Solution to organize Your Day</h1>
                    <h2 data-aos="fade-up" data-aos-delay="400">A Daily Helpful To-Do List</h2>
                </div>
                <div class="col-lg-6 hero-img" data-aos="zoom-out" data-aos-delay="200">
                    <img src="~/Home Scripts/assets/img/hero-img.png" class="img-fluid" alt="">
                </div>
            </div>
        </div>

    </section><!-- End Hero -->

    <main id="main">
        <!-- ======= About Section ======= -->
        <section id="about" class="about">

            <div class="container" data-aos="fade-up">
                <div class="row gx-0">

                    <div class="col-lg-6 d-flex flex-column justify-content-center" data-aos="fade-up" data-aos-delay="200">
                        <div class="content">
                            <h3>Who We Are?</h3>
                            <h2>We are a Team of Engineering students</h2>

                            <p style="font-size:20px">
                                We Have just started our journey in web Development and we hope that you find our product Useful
                            </p>
                            <div class="Size">
                                <div class="text-center text-lg-start">
                                    <a href="#" class="btn-read-more d-inline-flex align-items-center justify-content-center align-self-center">
                                        <span>Read More</span>
                                        <i class="bi bi-arrow-right"></i>
                                    </a>
                                </div>
                            </div>
                        </div>
                        <!-- Image -->
                        <!---div class="col-lg-6 d-flex align-items-center" data-aos="zoom-out" data-aos-delay="200">
                          <img src="~/Image/pexels-cottonbro-7438102.jpg" class="img-fluid" alt="" style="height:1500px;width:1000px">
                        </!--div>
                        <!-- Image -->

                    </div>
                </div>

        </section><!-- End About Section -->
        <!-- ======= Values Section ======= -->
        <section id="values" class="values">

            <div class="container" data-aos="fade-up">

                <header class="section-header">
                    <h2>Our Values</h2>
                    <p>Our Solution Was Based on this Values</p>
                </header>

                <div class="row">

                    <div class="col-lg-4" data-aos="fade-up" data-aos-delay="200">
                        <div class="box">
                            <img src="~/Home Scripts/assets/img/values-1.png" class="img-fluid" alt="">
                            <h3>Make Your Life Easier</h3>
                            <p style="font-size:20px">By Noting Your Daily Tasks You Will Avoid Getting in Troubles Because You forgot them.</p>
                        </div>
                    </div>

                    <div class="col-lg-4 mt-4 mt-lg-0" data-aos="fade-up" data-aos-delay="400">
                        <div class="box">
                            <img src="~/Home Scripts/assets/img/values-2.png" class="img-fluid" alt="">
                            <h3>Increase Your Productivity</h3>
                            <p style="font-size:20px">Organizing Your Tasks And Precising Your Goals Helps you To Be more Successful</p>
                        </div>
                    </div>

                    <div class="col-lg-4 mt-4 mt-lg-0" data-aos="fade-up" data-aos-delay="600">
                        <div class="box">
                            <img src="~/Home Scripts/assets/img/values-3.png" class="img-fluid" alt="">
                            <h3>Optimize Your Time.</h3>
                            <p style="font-size:20px">Time Optimization Is the Secret of the Successful Person.</p>
                        </div>
                    </div>

                </div>

            </div>

        </section><!-- End Values Section -->
        <!-- ======= Counts Section ======= -->
        <section id="counts" class="counts">
            <div class="container" data-aos="fade-up">

                <div class="row gy-4">

                    <div class="col-lg-3 col-md-6">
                        <div class="count-box">
                            <i class="bi bi-emoji-smile"></i>
                            <div>
                                <span data-purecounter-start="0" data-purecounter-end="50" data-purecounter-duration="1" class="purecounter"></span>
                                <p style="font-size:20px">Happy Clients</p>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6">
                        <div class="count-box">
                            <i class="bi bi-journal-richtext" style="color: #ee6c20;"></i>
                            <div>
                                <span data-purecounter-start="0" data-purecounter-end="50" data-purecounter-duration="1" class="purecounter"></span>
                                <p style="font-size:20px">Projects</p>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6">
                        <div class="count-box">
                            <i class="bi bi-headset" style="color: #15be56;"></i>
                            <div>
                                <span data-purecounter-start="0" data-purecounter-end="140" data-purecounter-duration="1" class="purecounter"></span>
                                <p style="font-size:20px">Hours Of Support</p>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6">
                        <div class="count-box">
                            <i class="bi bi-people" style="color: #bb0852;"></i>
                            <div>
                                <span data-purecounter-start="0" data-purecounter-end="5" data-purecounter-duration="1" class="purecounter"></span>
                                <p style="font-size:20px">Hard Workers</p>
                            </div>
                        </div>
                    </div>

                </div>

            </div>
        </section><!-- End Counts Section -->
        <!-- ======= Features Section ======= -->
        <!-- End Features Section -->
        <!-- ======= Services Section ======= -->
        <!-- End Pricing Section -->
        <!-- ======= F.A.Q Section ======= -->
        <section id="faq" class="faq">

            <div class="container" data-aos="fade-up">

                <header class="section-header">
                    <h2>F.A.Q</h2>
                    <p>Frequently Asked Questions</p>
                </header>

                <div class="row">
                    <div class="col-lg-6">
                        <!-- F.A.Q List 1-->
                        <div class="accordion accordion-flush" id="faqlist1">
                            <div class="accordion-item">
                                <h2 class="accordion-header">
                                    <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#faq-content-1">
                                        How Can I Use the List?
                                    </button>
                                </h2>
                                <div id="faq-content-1" class="accordion-collapse collapse" data-bs-parent="#faqlist1">
                                    <div class="accordion-body">
                                        <p style="font-size:20px">You Can Add Items or Tasks To The List by Writing them in the Text Box. Hit Enter AND VOILAAA!!! Your Task is There</p>
                                    </div>
                                </div>
                            </div>

                            <div class="accordion-item">
                                <h2 class="accordion-header">
                                    <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#faq-content-2">
                                        Can I edit Items After adding them in the List?
                                    </button>
                                </h2>
                                <div id="faq-content-2" class="accordion-collapse collapse" data-bs-parent="#faqlist1">
                                    <div class="accordion-body">
                                        <p style="font-size:20px">Of Course you Can .Just press The Edit Button next to the Item you want to modify, you will be redirected to another page, make your modifications and click save to go back to the list.</p>
                                    </div>
                                </div>
                            </div>

        </section><!-- End F.A.Q Section -->
        <!-- ======= Team Section ======= -->
        <section id="team" class="team">

            <div class="container" data-aos="fade-up">

                <header class="section-header">
                    <h2>Team</h2>
                    <p>Our hard working team</p>
                </header>

                <div class="row gy-4">

                    <div class="member">
                        <div class="member-info">
                            <h4>Zakaria BAQASSE</h4>
                        </div>
                    </div>

                    <div class="member">
                        <div class="member-info">
                            <h4>Hiba DOUIK</h4>
                        </div>
                    </div>

                    <div class="member">
                        <div class="member-info">
                            <h4>Ouijdane BEN-YADI</h4>
                        </div>
                    </div>

                    <div class="member">
                        <div class="member-info">
                            <h4>Dina ELHYATE </h4>
                        </div>
                    </div>
                    <div class="member">
                        <div class="member-info">
                            <h4>Yassine ELJABRI</h4>
                        </div>
                    </div>

                </div>

            </div>

        </section><!-- End Team Section -->
        <!-- ======= Clients Section ======= -->
        <!-- End Recent Blog Posts Section -->
        <!-- ======= Contact Section ======= -->
        <!-- End Contact Section -->

    </main><!-- End #main -->
    <!-- ======= Footer ======= -->
    <!-- End Footer -->

    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Vendor JS Files -->
    <script src="~/Home Scripts/assets/vendor/purecounter/purecounter.js"></script>
    <script src="~/Home Scripts/assets/vendor/aos/aos.js"></script>
    <script src="~/Home Scripts/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Home Scripts/assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/Home Scripts/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/Home Scripts/assets/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/Home Scripts/assets/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="~/Home Scripts/assets/js/main.js"></script>

</body>
@Styles.Render("~/Content/css")
@Scripts.Render("~/bundles/modernizr")

